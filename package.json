    "Calculator"
  ],
  "author": "Bruno Passos <brunopassos.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bpassos/salaris/issues"
  },
  "homepage": "https://github.com/bpassos/salaris#readme",
  "devDependencies": {
    "babelify": "^6.3.0",
    "browserify": "^11.2.0",
    "connect": "^3.4.0",
    "serve-static": "^1.10.0"
  }
}
