{
  "name": "salaris",
  "version": "0.0.1",
  "description": "Dutch salary calculator",
  "main": "index.js",
  "scripts": {
    "start": "node server.js",
    "watch": "watchify scripts/index.js -o index.js -dv",
    "build": "browserify scripts/index.js | uglifyjs -mc > index.js"
